 <%-include('../partials/usernave.ejs')-%>
  

 <section class="" style="padding: 250px;">
        <div class="container">
            <div class="row" style="margin: -130px;">
                <!-- sidebar -->
    
                <!-- content -->
                  <div class="container d-flex" style="margin-top: 81px;">
  
    

          
                      <% if(SearchResutl.length==0){ %>
                             <div style="display: flex; justify-content: center; margin-left: 448px;">
                            <div style="text-align: center;">
                       <img src="/assets/Search.png" alt="SearchImage">
                     <h3>Sorry, no results found! </h3>
                    <p>Please check the spelling or try searching for something else</p>
                     </div>
                    </div>


                      </div>
<% } else{ %>
    
                  
    
                    <div class="row" id="gridViewSection">
                        <% SearchResutl.forEach(data => { %>
                            <div class="col-lg-3 col-md-6 col-sm-6 my-2">
                                <div class="card shadow-2-strong">
                                    <img src="/productimages/<%= data.images[0] %>" class="card-img-top" />
                                    <div class="card-body">
                                        <h6 style="font-size: 18px;"><%-data.productName-%></h6><br>
                                        <div class="d-flex flex-row">
                                            <h6 class="mb-1 me-1">₹ <%-data.DiscountAmount-%></h6>
                                            <span class="text-danger"><s>₹ <%-data.Price-%></s></span>
                                        </div>
    
                                      
    
                                        <div class="d-flex justify-content-end" style="margin-top: 5px;">
                                        
                                                <a href="/singleproducts/<%-data._id-%>" class="btn btn-outline-primary shadow-0 me-1" >View</a>
                                          
                                            <a class="btn btn-light border  icon-hover px-2 pt-2" onclick="addToWishlist('<%-data._id-%>')"><i class="fas fa-heart fa-lg text-secondary px-1"></i></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <% }) %>
                    </div>
                    <% } %>
    
                
            </div>
        </div>
    </section>
  <%-include('../partials/footernav.ejs')-%>
    <!-- jQuery -->
    <script src="/js/jquery-2.1.0.min.js"></script>

