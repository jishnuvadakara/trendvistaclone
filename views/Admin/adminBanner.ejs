  <%-include('../layout/admin/header.ejs')-%>
<%-include('../partials/adminnave.ejs')-%>

  <section style="background-color: #eee;">
    <div style="display: flex; justify-content: start;">
      <a href="/admin/AddBanner"><button type="button" class="btn btn-primary btn-lg btn-block">
                   Add Banner
                  </button></a>
    </div>
  <div class="container py-5">
  <div class="row justify-content-center mb-3">
    <% BannerUI.forEach(ele => { %>
    <div class="col-md-12 ">
      <div class="card shadow-0 border rounded-3">
        <div class="card-body">
          <div class="row">
            <div class="col-md-12 ">
              <div class="bg-image hover-zoom ripple rounded ripple-surface">
                <img src="/banner-images/<%= ele.bannerImages %>"
                  class="w-100" />
                <a href="#!">
                  <div class="hover-overlay">
                    <div class="mask" style="background-color: rgba(253, 253, 253, 0.15);"></div>
                  </div>
                </a>
              </div>
            </div>
            <div class="col-md-6 col-lg-6 col-xl-6">
              <!-- Other card content -->
                   <div class=" mb-1" style="display: flex;justify-content: end;">
                  <h4 class="mb-1 me-1"><%-ele.bannerName-%></h4>
                  
                </div>
                   
                <div class="d-flex flex-column mt-4">
                <a href="/" >  <button type="button" class="btn btn-primary btn-lg btn-block">View Banner UI</button></a>
                  <button class="btn btn-outline-primary btn-sm mt-2" type="button">
                    Add 
                  </button>
            </div>
            <div class="col-md-6 col-lg-3 col-xl-3 border-sm-start-none border-start">
              <!-- Other card content -->
            </div>
          </div>
        </div>
      </div>
    </div>
    <% }) %>
  </div>
</div>

</section>
</html>
